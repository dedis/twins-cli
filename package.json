{
  "name": "researchercli",
  "description": "CLI to demonstrate a researcher's workflow in TWINS",
  "version": "0.0.0",
  "author": "Gaurav Narula @gnarula",
  "bin": {
    "researchercli": "./bin/run"
  },
  "bugs": "https://github.com/dedis/twins-cli/issues",
  "dependencies": {
    "@dedis/cothority": "../cothority/external/js/cothority/dist",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "aries-framework-javascript": "../aries-framework-javascript/aries-framework-javascript-v1.0.3.tgz",
    "indy-sdk": "^1.14.2",
    "node-fetch": "^2.6.0",
    "qrcode": "^1.4.4",
    "tslib": "^1",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "@types/node-fetch": "^2.5.4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/dedis/twins-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "researchercli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "dedis/twins-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
